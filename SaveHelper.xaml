<Window x:Class="QualitySystem.SaveHelper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Сохранить..." Height="170" Width="300">
    <DockPanel>
        <Label Content="Выберите требуемое действие" HorizontalAlignment="Center" DockPanel.Dock="Top" />
        <RadioButton Name="radioReg" Content="Сохранить регрессионную модель" Margin="5,0" DockPanel.Dock="Top"/>
        <RadioButton Name="radioDes" Content="Сохранить дискретную модель" Margin="5,2" DockPanel.Dock="Top"/>
        <RadioButton Name="radioAll" Content="Сохранить все новые построенные модели" Margin="5,0" DockPanel.Dock="Top"/>
        <Button Name="btnOk" Content="OK" Margin="5,10" Click="btnOk_Click" />
    </DockPanel>
</Window>
